{
  "name": "glints-aries",
  "version": "4.0.70",
  "description": "Glints ui-kit for frontend",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:icon && npm run build:lib && npm run build:es && npm run build:ts",
    "build:lib": "rm -rf lib && babel src -d lib --extensions .ts,.tsx --ignore src/**/*.test.tsx",
    "build:es": "rm -rf es && cross-env BABEL_ENV=es babel src -d es --extensions .ts,.tsx --ignore src/**/*.test.tsx",
    "build:ts": "tsc --rootDir src -d --declarationDir lib && tsc --rootDir src -d --declarationDir es",
    "build:icon": "rm -rf src/General/Icon/components && babel-node --extensions .ts,.tsx src/Utils/Icon/generateIcons.ts && npm run format:icon",
    "check:branch": "if [ \"$(git rev-parse --abbrev-ref HEAD)\" = v4 ] ; then echo Branch check passed. ; else echo Error: Not on branch v4. ; exit 1 ; fi",
    "format:icon": "prettier --write src/General/Icon/components/*.tsx",
    "lint": "npm run lint:css && npm run lint:ts",
    "lint:css": "stylelint 'src/Style/**/*.ts'",
    "lint:ts": "eslint 'src/**/*.{ts,tsx}'",
    "preversion": "npm run check:branch && git pull && npm whoami && npm run validate",
    "postversion": "git push && git push --tags && npm publish",
    "prepublishOnly": "npm run check:branch && git pull && npm whoami && npm run validate",
    "storybook": "npm run storybook:build:assets && npm run build:icon && start-storybook -p 9001 -c storybook-config -s dist/public",
    "storybook:build": "rm -rf .out && npm run storybook:build:assets && npm run build:icon && build-storybook -c storybook-config -o .out -s dist/public",
    "storybook:build:assets": "webpack",
    "test:update": "jest -u",
    "test": "jest",
    "validate": "npm run build && npm run lint && npm run test"
  },
  "author": "Glints",
  "license": "MIT",
  "private": false,
  "keywords": [
    "react",
    "glints-aries",
    "glints-uikit"
  ],
  "bugs": {
    "url": "https://github.com/glints-dev/glints-aries/issues"
  },
  "homepage": "http://aries.glints.design",
  "files": [
    "assets",
    "es",
    "lib",
    "dist",
    "*.d.ts"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@storybook/theming": "^5.1.11",
    "@testing-library/dom": "^6.1.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.3",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.8",
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.132",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.12.7",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.9.0",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "^5.0.0",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "create-index": "^2.4.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "generate-changelog": "^1.7.1",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.1",
    "jsx-to-string": "^1.4.0",
    "prettier": "1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "styled-components": "^4.2.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.2",
    "webpack-glob-entry": "2.1.1"
  },
  "dependencies": {
    "@glints/poppins": "^1.0.0",
    "classnames": "^2.2.6",
    "create-react-context": "^0.2.3",
    "moment": "^2.24.0",
    "r": "0.0.5",
    "react-datetime": "^2.16.3",
    "react-responsive": "^6.1.2"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "styled-components": "^4.0.0"
  }
}
