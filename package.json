{
  "name": "glints-aries",
  "version": "4.0.132",
  "description": "Glints ui-kit for frontend",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:icon && npm run build:lib && npm run build:es && npm run build:ts",
    "build:lib": "rm -rf lib && babel src -d lib --extensions .ts,.tsx --ignore src/**/*.test.tsx,src/**/*.test.ts",
    "build:es": "rm -rf es && cross-env BABEL_ENV=es babel src -d es --extensions .ts,.tsx --ignore src/**/*.test.tsx,src/**/*.test.ts",
    "build:ts": "tsc --project tsconfig.declaration.json -d --declarationDir lib && tsc --project tsconfig.declaration.json -d --declarationDir es",
    "build:icon": "rm -rf src/General/Icon/components && babel-node --extensions .ts,.tsx src/Utils/Icon/generateIcons.ts && npm run format:icon",
    "format:icon": "prettier --write src/General/Icon/components/*.tsx",
    "imports": "babel-node --extensions .ts scripts/count-imports/index.ts",
    "lint": "npm run lint:css && npm run lint:ts",
    "lint:css": "stylelint '**/*Style.ts'",
    "lint:ts": "eslint '**/*.{ts,tsx}'",
    "test:update": "jest -u",
    "test": "jest",
    "coverage:src": "jest --coverage --collectCoverageFrom 'src/**/*.tsx' --coveragePathIgnorePatterns 'src/General/Icon/components/' --collectCoverageFrom='!src/**/*.stories.tsx' --collectCoverageFrom='!src/**/*StoryStyle.tsx' --collectCoverageFrom='!src/**/*StoriesHelpers.tsx' --collectCoverageFrom='!src/stories/story-components/**'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "rm -rf .out && npm run storybook:build:assets && npm run build:icon && build-storybook -o .out -s dist/public",
    "storybook:build:assets": "webpack"
  },
  "author": "Glints",
  "license": "MIT",
  "private": false,
  "keywords": [
    "react",
    "glints-aries",
    "glints-uikit"
  ],
  "bugs": {
    "url": "https://github.com/glints-dev/glints-aries/issues"
  },
  "homepage": "http://aries.glints.design",
  "files": [
    "assets",
    "es",
    "lib",
    "dist",
    "*.d.ts"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@testing-library/dom": "^6.1.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.3",
    "@testing-library/user-event": "^7.1.2",
    "@types/async": "^3.2.0",
    "@types/classnames": "^2.2.8",
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.12.7",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.9.0",
    "@types/styled-components": "^5.1.1",
    "@types/styled-system": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "async": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "babylon7": "npm:@babel/parser@^7",
    "chromatic": "^5.4.0",
    "create-index": "^2.4.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "generate-changelog": "^1.7.1",
    "glob": "^7.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-styled-components": "^7.0.2",
    "jsx-to-string": "^1.4.0",
    "lodash": "^4.17.15",
    "prettier": "1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-id-generator": "^3.0.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.8.6",
    "styled-components": "^5.1.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.2",
    "webpack-glob-entry": "2.1.1"
  },
  "dependencies": {
    "@types/body-scroll-lock": "^2.6.1",
    "body-scroll-lock": "^3.1.5",
    "classnames": "^2.2.6",
    "create-react-context": "^0.2.3",
    "downshift": "^6.1.3",
    "moment": "^2.24.0",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "styled-components": "^4.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js"
    }
  },
  "optionalDependencies": {
    "@glints/poppins": "^1.0.2"
  }
}
